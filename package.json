{
  "name": "jianghu-ui",
  "version": "1.0.1",
  "description": "JianghuJS UI Component Library with Storybook, Vue 2, and Vuetify 2",
  "main": "src/index.js",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch",
    "serve-storybook": "npx http-server storybook-static",
    "deploy": "npm run build && npm run build-storybook && npx gh-pages -d storybook-static"
  },
  "keywords": [
    "jianghujs",
    "vue",
    "vuetify",
    "ui-components",
    "storybook"
  ],
  "author": "JianghuJS Team",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-storysource": "^7.6.20",
    "@storybook/blocks": "^7.6.17",
    "@storybook/vue-webpack5": "^7.6.17",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.1.1",
    "http-server": "^14.1.1",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.2.0",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.16",
    "vue": "^2.7.16",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vuetify": "^2.7.0"
  }
}
